Panel Configurations:
  - Type: rviz_common/Displays
    Properties:
      Name: Displays
      Help Height: 78
      Property Tree Widget:
        Expanded Items:
          - /Global Options
          - /Status
          - /Quori
          - /TF2
        Splitter Ratio: 0.5
      Tree Height: 657

  - Class: rviz_common/SelectionPanel
    Properties:
      Name: Selection

  - Class: rviz_common/Tool Properties
    Properties:
      Name: Tool Properties
      Expanded Tools:
        - /2D Pose Estimate
        - /2D Nav Goal
        - /Publish Point
      Splitter Ratio: 0.588679

  - Class: rviz_common/Views
    Properties:
      Name: Views
      Expanded Items:
        - /Current View
      Splitter Ratio: 0.5

  - Class: rviz_common/TimePanel
    Properties:
      Name: Time
      Sync Mode: 0
      Sync Source: ""

Visualization Manager:
  Displays:
    - Type: rviz_default_plugins/Grid
      Properties:
        Alpha: 0.5
        Cell Size: 1
        Color: 160, 160, 164
        Enabled: true
        Line Style: Lines
        Line Width: 0.03
        Name: Grid
        Plane: XY
        Plane Cell Count: 10
        Reference Frame: world

