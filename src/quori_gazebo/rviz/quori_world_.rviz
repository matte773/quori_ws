    - Type: rviz_default_plugins/Displays/Image
      Properties:
        Name: Image
        Image Topic: /camera/image_raw  # Adjust according to your ROS2 topic
        Enabled: false
        Transport Hint: raw
        Queue Size: 2
        Normalize Range: true
        Min Value: 0
        Max Value: 1
        Median window: 5
        Unreliable: false

Global Options:
  Properties:
    Background Color: 85, 87, 83
    Fixed Frame: world
    Frame Rate: 30

Tools:
  - Type: rviz_default_plugins/Tools/Interact
    Properties: 
      Hide Inactive Objects: true
  - Type: rviz_default_plugins/Tools/MoveCamera
  - Type: rviz_default_plugins/Tools/Select
  - Type: rviz_default_plugins/Tools/FocusCamera
  - Type: rviz_default_plugins/Tools/Measure
  - Type: rviz_default_plugins/Tools/SetInitialPose
    Properties:
      Topic: /initialpose
      X std deviation: 0.5
      Y std deviation: 0.5
      Theta std deviation: 0.261799
  - Type: rviz_default_plugins/Tools/SetGoal
    Properties:
      Topic: /move_base_simple/goal

Preferences:
  PromptSaveOnExit: true

Toolbars:
  toolButtonStyle: 2

Window Geometry:
  # This part is heavily dependent on the specific layout and size of your RViz2 window and may require manual adjustment.
  Height: 960
  Width: 1208
  X: 72
  Y: 27
  Displays:
    collapsed: false
  Selection:
    collapsed: false
  Time:
    collapsed: false
  Tool Properties:
    collapsed: false
  Views:
    collapsed: false

